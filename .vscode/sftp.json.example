{
  "name": "Production Server - events.stepperslife.com",
  "host": "YOUR_SERVER_IP",
  "protocol": "sftp",
  "port": 22,
  "username": "YOUR_USERNAME",
  "password": "YOUR_PASSWORD",
  "remotePath": "/root/websites/event.stepperslife.com",
  "uploadOnSave": false,
  "useTempFile": false,
  "openSsh": false,
  "downloadOnOpen": false,
  "ignore": [
    ".vscode",
    ".git",
    ".DS_Store",
    "node_modules",
    ".env.local",
    ".next",
    "*.log"
  ],
  "watcher": {
    "files": "**/*",
    "autoUpload": false,
    "autoDelete": false
  },
  "syncOption": {
    "delete": false,
    "skipCreate": false,
    "ignoreExisting": false,
    "update": true
  }
}
